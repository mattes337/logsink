# LogSink v2.1 Environment Configuration

# Server Configuration
PORT=1234
HOST=localhost
API_KEY=super-secret-log-api-key-2024

# PostgreSQL Database Configuration
DB_HOST=localhost
DB_PORT=5432
DB_NAME=logsink
DB_USER=logsink
DB_PASSWORD=logsink

# Database Connection Pool Settings
DB_POOL_MAX=20
DB_IDLE_TIMEOUT=30000
DB_CONNECTION_TIMEOUT=2000
DB_SSL=false

# Storage Configuration
IMAGES_DIR=./images
MAX_IMAGE_SIZE=10485760
ALLOWED_IMAGE_TYPES=jpg,jpeg,png,gif,webp,bmp

# AI Integration (Google Gemini)
GEMINI_API_KEY=
GEMINI_MODEL=gemini-1.5-flash
GEMINI_MAX_TOKENS=8192
GEMINI_TEMPERATURE=0.7
GEMINI_ENABLED=false

# Gemini Embedding Configuration
GEMINI_EMBEDDING_ENABLED=false
GEMINI_EMBEDDING_MODEL=text-embedding-004
GEMINI_SIMILARITY_THRESHOLD=0.85

# Blacklist Configuration
BLACKLIST_ENABLED=true
BLACKLIST_AUTO_DELETE=false
BLACKLIST_CACHE_TIMEOUT=300000

# Cleanup Service Configuration
CLEANUP_ENABLED=true
CLEANUP_INTERVAL=0 2 * * *
DUPLICATE_THRESHOLD=0.85
CLEANUP_MAX_AGE=2592000000
CLEANUP_BATCH_SIZE=100

# Logging Configuration
LOG_LEVEL=info
LOG_FORMAT=combined
LOG_FILE=

# CORS Configuration
CORS_ORIGIN=*
CORS_METHODS=GET, POST, PUT, DELETE, PATCH
CORS_HEADERS=Content-Type, Authorization, X-API-Key

# Development/Production Mode
NODE_ENV=development